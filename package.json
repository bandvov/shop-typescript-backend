    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.12.7",
    "sha256": "^0.2.0",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6"
